                "--learning_start", "100",
                // "--debug",
                // Additional config
                "--pause_cost", "-0.1",
                "--use_pause_env",
                "--action_repeat", "5",
                "--evaluator", 
                "/home/niko/Repos/atari-cr/output/atari_head/ms_pacman/drout0.3/999/checkpoint.pth",
                "--fov", "window",
                "--fov_size", "32",
                "--timed_env",
                "--td_steps", "4",
                "--gamma", "0.95",
                "--sensory_action_x_size", "8",
                "--sensory_action_y_size", "8",
            ],
            "justMyCode": false,
            "env": {"PYDEVD_DISABLE_FILE_VALIDATION": "1"}
        },
        {
            "name": "DQN CR OG",
            "type": "debugpy",
