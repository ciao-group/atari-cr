        {
            "name": "Gaze Prediction",
            "type": "debugpy",
            "request": "launch",
            "program": "src/atari_cr/atari_head/gaze_predictor.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--debug",
                "--load_model",
                "--n", "1",
                "--eval_train_data",
                // "--load_saliency",
                "--unet"
            ]
        }, {
            "name": "Ray",
            "type": "debugpy",
            "request": "launch",
            "program": "src/atari_cr/hyperparams.py",
            "console": "integratedTerminal",
        },
        {
            "name": "ViT",
            "type": "debugpy",
            "request": "launch",
            "program": "src/atari_cr/atari_head/vit.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--debug",
                "--load_saliency"
            ]
        },
        {
            "name": "DQN CR",
            "type": "debugpy",
            "request": "launch",
            "program": "src/atari_cr/agents/dqn_atari_cr/main.py",
            "console": "integratedTerminal",
            "pythonArgs": [
                // Not having this displays a warning because frozen modules bad
                "-Xfrozen_modules=off"
